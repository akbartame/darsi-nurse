# BLE Gateway Configuration

# MQTT Broker Settings
VITE_MQTT_BROKER=ws://broker.hivemq.com:8000/mqtt
VITE_MQTT_TOPIC=ble-gateway/data
VITE_MQTT_USERNAME=
VITE_MQTT_PASSWORD=

# Alternative MQTT Brokers (uncomment to use)
# Eclipse Mosquitto
# VITE_MQTT_BROKER=ws://test.mosquitto.org:8080/mqtt

# Eclipse IoT
# VITE_MQTT_BROKER=ws://mqtt.eclipseprojects.io:80/mqtt

# Local MQTT Broker (if running locally)
# VITE_MQTT_BROKER=ws://localhost:9001/mqtt

# BLE Device Filters (optional)
# VITE_BLE_NAME_PREFIX=ESP
# VITE_BLE_SERVICE_UUID=battery_service

# Debug Mode
VITE_DEBUG_MODE=true